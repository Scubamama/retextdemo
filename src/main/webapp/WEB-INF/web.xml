<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>ReText Book Buy/Sell</display-name>
  

  <servlet>
  	<servlet-name>RetextManagerServlet</servlet-name>
  	<servlet-class>servlets.RetextManagerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RetextTitleLocatedServlet</servlet-name>
  	<display-name>RetextTitleLocatedServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.RetextTitleLocatedServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RetextLoginServlet</servlet-name>
  	<servlet-class>servlets.RetextLoginServlet</servlet-class>
  </servlet>
    <servlet>
  	<servlet-name>RetextCreateUserServlet</servlet-name>
  	<servlet-class>servlets.RetextCreateUserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>RetextManagerServlet</servlet-name>
  	<url-pattern>/manage/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RetextTitleLocatedServlet</servlet-name>
  	<url-pattern>/located/*</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
  	<servlet-name>RetextLoginServlet</servlet-name>
  	<url-pattern>/login/*</url-pattern>
  </servlet-mapping>
      <servlet-mapping>
  	<servlet-name>RetextCreateUserServlet</servlet-name>
  	<url-pattern>/newUser/*</url-pattern>
  </servlet-mapping>


</web-app>
