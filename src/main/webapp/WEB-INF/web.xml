<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>ReText Book Buy/Sell</display-name>
  
  <servlet>
  	<servlet-name>retextManagerServlet</servlet-name>
  	<display-name>retextManagerServlet</display-name>
  	<description></description>
  	<servlet-class>retextdemo.retextManagerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RetextManagerServlet</servlet-name>
  	<display-name>RetextManagerServlet</display-name>
  	<description></description>
  	<servlet-class>retextdemo.RetextManagerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>retextManagerServlet</servlet-name>
  	<url-pattern>/retextManagerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RetextManagerServlet</servlet-name>
  	<url-pattern>/RetextManagerServlet</url-pattern>
  </servlet-mapping>
</web-app>
